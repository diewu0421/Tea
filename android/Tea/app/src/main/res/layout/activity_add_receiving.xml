<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>

    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!--top start -->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_44dp"
            android:background="@color/bg_color_f4f4f4">

            <ImageView
                android:id="@+id/good_list_goback"
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_5dp"
                android:background="@mipmap/btn_fangx"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/add_receiving_address"
                android:textColor="@color/color_191919"
                android:textSize="@dimen/sp_17"

                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textColor="@color/color_ff6243"
                android:textSize="@dimen/sp_14"
                android:text="@string/save"
                android:id="@+id/collect_good_edit"
                android:layout_marginRight="@dimen/margin_20dp"
                />
        </RelativeLayout>
        <!--top end -->

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_45dp"
            android:background="@drawable/border_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_11dp"
                android:text="@string/receiving_person"
                android:textSize="@dimen/sp_14"
                />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_122dp"
                android:textColor="@color/color_acacac"
                android:textSize="@dimen/sp_14"
                android:background="@null"
                android:hint="请输入收货人姓名"
                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_45dp"
            android:background="@drawable/border_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_11dp"
                android:text="@string/contact_tel"
                android:textSize="@dimen/sp_14"
                />

            <EditText
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_122dp"
                android:textColor="@color/color_acacac"
                android:textSize="@dimen/sp_14"
                android:background="@null"
                android:hint="请输入11位电话号码"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_45dp"
            android:background="@drawable/border_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_11dp"
                android:text="@string/diqu"
                android:textSize="@dimen/sp_14"
                />


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/margin_20dp"
                android:layout_centerVertical="true">



                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/please_select"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/color_222222"
                    android:layout_gravity="center_vertical"
                    />

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:background="@mipmap/btn_fangxiang"

                    />

            </LinearLayout>


        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:background="@drawable/border_bottom_16px"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:paddingTop="20dp"
            >
            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerVertical="true"
                android:textColor="@color/color_acacac"
                android:textSize="@dimen/sp_14"
                android:background="@null"
                android:hint="请填写详细地址，不少于5个字"
                android:gravity="top"
                android:padding="10dp"

                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_45dp"
            android:background="@drawable/border_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/margin_20dp"
                android:background="@mipmap/btn_quan"

                android:id="@+id/imageView3"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="11dp"
                android:text="@string/set_default_address"
                android:textSize="@dimen/sp_14"
                android:layout_alignTop="@+id/imageView3"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="11dp"
                android:id="@+id/textView4"/>
        </RelativeLayout>


    </LinearLayout>
</layout>