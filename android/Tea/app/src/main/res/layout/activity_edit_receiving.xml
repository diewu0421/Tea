<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>

        <variable
            name="rvLinearManager"
            type="android.support.v7.widget.LinearLayoutManager"/>

        <variable
            name="rvAdapter"
            type="com.gshl.tea.adapter.CommonRVAdapter"/>
    </data>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!--top start -->
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_44dp"
            android:background="@color/bg_color_f4f4f4">

            <ImageView
                android:id="@+id/good_list_goback"
                android:layout_width="23dp"
                android:layout_height="23dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_5dp"
                android:background="@mipmap/btn_fangx"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/edit_receiving_address"
                android:textColor="@color/color_191919"
                android:textSize="@dimen/sp_17"

                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:textColor="@color/color_ff6243"
                android:textSize="@dimen/sp_14"
                android:text="@string/save"
                android:id="@+id/collect_good_edit"
                android:layout_marginRight="@dimen/margin_20dp"
                />
        </RelativeLayout>
        <!--top end -->

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_45dp"
            android:background="@drawable/border_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_11dp"
                android:text="@string/receiving_person"
                android:textSize="@dimen/sp_14"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_122dp"
                android:text="鲁人甲"
                android:textColor="@color/color_222222"
                android:textSize="@dimen/sp_14"

                />

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_45dp"
            android:background="@drawable/border_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_11dp"
                android:text="@string/contact_tel"
                android:textSize="@dimen/sp_14"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_122dp"
                android:text="15856591558"
                android:textColor="@color/color_222222"
                android:textSize="@dimen/sp_14"

                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_45dp"
            android:background="@drawable/border_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_11dp"
                android:text="@string/diqu"
                android:textSize="@dimen/sp_14"
                />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_122dp"
                android:text="广东深深圳市宝安区"
                android:textColor="@color/color_222222"
                android:textSize="@dimen/sp_14"

                />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/margin_20dp"
                android:background="@mipmap/btn_fangxiang"

                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:background="@drawable/border_bottom_16px"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:paddingTop="20dp"
            >
            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@null"
                android:text="西乡街道永乐新村"
                android:textSize="@dimen/sp_14"
                android:textColor="@color/color_222222"
                android:paddingLeft="18dp"
                android:gravity="top"
                />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_45dp"
            android:background="@drawable/border_bottom"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_left_11dp"
                android:text="@string/set_default_address"
                android:textSize="@dimen/sp_14"
                />

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/margin_20dp"
                android:background="@mipmap/btn_quan"

                />
        </RelativeLayout>


    </LinearLayout>
</layout>