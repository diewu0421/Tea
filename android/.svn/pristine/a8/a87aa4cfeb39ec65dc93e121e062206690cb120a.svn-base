    @Override
    public boolean hasPendingBindings() {
        synchronized(this) {
            if (mDirtyFlags != 0) {
                return true;
            }
        }
        return false;
    }

    public boolean setVariable(int variableId, Object variable) {
        switch(variableId) {
            case BR.history :
                setHistory((com.gshl.tea.module.good.bean.History) variable);
                return true;
        }
        return false;
    }

    public void setHistory(com.gshl.tea.module.good.bean.History history) {
        updateRegistration(0, history);
        this.mHistory = history;
        synchronized(this) {
            mDirtyFlags |= 0x1L;
        }
        notifyPropertyChanged(BR.history);
        super.requestRebind();
    }
    public com.gshl.tea.module.good.bean.History getHistory() {
        return mHistory;
    }

    @Override
    protected boolean onFieldChange(int localFieldId, Object object, int fieldId) {
        switch (localFieldId) {
            case 0 :
                return onChangeHistory((com.gshl.tea.module.good.bean.History) object, fieldId);
        }
        return false;
    }
    private boolean onChangeHistory(com.gshl.tea.module.good.bean.History history, int fieldId) {
        switch (fieldId) {
            case BR.content: {
                synchronized(this) {
                        mDirtyFlags |= 0x2L;
                }
                return true;
            }
            case BR._all: {
                synchronized(this) {
                        mDirtyFlags |= 0x1L;
                }
                return true;
            }
        }
        return false;
    }

    @Override
    protected void executeBindings() {
        long dirtyFlags = 0;
        synchronized(this) {
            dirtyFlags = mDirtyFlags;
            mDirtyFlags = 0;
        }
        com.gshl.tea.module.good.bean.History history = mHistory;
        java.lang.String contentHistory = null;

        if ((dirtyFlags & 0x7L) != 0) {



                if (history != null) {
                    // read history.content
                    contentHistory = history.getContent();
                }
        }
        // batch finished
        if ((dirtyFlags & 0x7L) != 0) {
            // api target 1

            android.databinding.adapters.TextViewBindingAdapter.setText(this.mboundView1, contentHistory);
        }
    }
