    public SystemNotiItemBinding(android.databinding.DataBindingComponent bindingComponent, View root) {
        super(bindingComponent, root, 0);
        final Object[] bindings = mapBindings(bindingComponent, root, 3, sIncludes, sViewsWithIds);
        this.mboundView0 = (android.widget.LinearLayout) bindings[0];
        this.mboundView0.setTag(null);
        this.mboundView1 = (android.widget.TextView) bindings[1];
        this.mboundView1.setTag(null);
        this.mboundView2 = (android.widget.TextView) bindings[2];
        this.mboundView2.setTag(null);
        setRootTag(root);
        // listeners
        invalidateAll();
    }

    @Override
    public void invalidateAll() {
        synchronized(this) {
                mDirtyFlags = 0x2L;
        }
        requestRebind();
    }

    @Override
    public boolean hasPendingBindings() {
        synchronized(this) {
            if (mDirtyFlags != 0) {
                return true;
            }
        }
        return false;
    }

    public boolean setVariable(int variableId, Object variable) {
        switch(variableId) {
            case BR.systemNotification :
                setSystemNotification((com.gshl.tea.module.home.bean.SystemNotification) variable);
                return true;
        }
        return false;
    }

    public void setSystemNotification(com.gshl.tea.module.home.bean.SystemNotification systemNotification) {
        this.mSystemNotification = systemNotification;
        synchronized(this) {
            mDirtyFlags |= 0x1L;
        }
        notifyPropertyChanged(BR.systemNotification);
        super.requestRebind();
    }
    public com.gshl.tea.module.home.bean.SystemNotification getSystemNotification() {
        return mSystemNotification;
    }

    @Override
    protected boolean onFieldChange(int localFieldId, Object object, int fieldId) {
        switch (localFieldId) {
        }
        return false;
    }

    @Override
    protected void executeBindings() {
        long dirtyFlags = 0;
        synchronized(this) {
            dirtyFlags = mDirtyFlags;
            mDirtyFlags = 0;
        }
        java.lang.String notificationContentS = null;
        com.gshl.tea.module.home.bean.SystemNotification systemNotification = mSystemNotification;
        java.lang.String notificationTimeSyst = null;

        if ((dirtyFlags & 0x3L) != 0) {



                if (systemNotification != null) {
                    // read systemNotification.notificationContent
                    notificationContentS = systemNotification.getNotificationContent();
                    // read systemNotification.notificationTime
                    notificationTimeSyst = systemNotification.getNotificationTime();
                }
        }
        // batch finished
        if ((dirtyFlags & 0x3L) != 0) {
            // api target 1

            android.databinding.adapters.TextViewBindingAdapter.setText(this.mboundView1, notificationTimeSyst);
            android.databinding.adapters.TextViewBindingAdapter.setText(this.mboundView2, notificationContentS);
        }
    }
    // Listener Stub Implementations
    // callback impls
    // dirty flag
    private  long mDirtyFlags = 0xffffffffffffffffL;

